@article{simba_application_2004,
	title = {Application of {ICT} in strengthening health information systems in developing countries in the wake of globalisation},
	volume = {4},
	issn = {1680-6905 (Print) 1680-6905 (Linking)},
	url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-19644395912&partnerID=40&md5=70d8fc1bf5122fb4b6b7c01e24b50e22},
	abstract = {Information Communication Technology (ICT) revolution brought opportunities and challenges to developing countries in their efforts to strengthen the Health Management Information Systems (HMIS). In the wake of globalisation, developing countries have no choice but to take advantage of the opportunities and face the challenges. The last decades saw developing countries taking action to strengthen and modernise their HMIS using the existing ICT. Due to poor economic and communication infrastructure, the process has been limited to national and provincial/region levels leaving behind majority of health workers living in remote/rural areas. Even those with access do not get maximum benefit from ICT advancements due to inadequacies in data quality and lack of data utilisation. Therefore, developing countries need to make deliberate efforts to address constraints threatening to increase technology gap between urban minority and rural majority by setting up favourable policies and appropriate strategies. Concurrently, strategies to improve data quality and utilisation should be instituted to ensure that HMIS has positive impact on people's health. Potential strength from private sector and opportunities for sharing experiences among developing countries should be utilised. Short of this, advancement in ICT will continue to marginalise health workers in developing countries especially those living in remote areas.},
	number = {3},
	journal = {African Health Sciences},
	author = {Simba, Daudi O and Mwangu, Mughwira},
	year = {2004},
	pages = {194--198},
	file = {Simba, Mwangu - 2004 - Application of ICT in strengthening health information systems in developing countries in the wake of globalisati:/home/gregoire/.zotero/zotero/ttzhcdaf.default/zotero/storage/WDDIH9GK/Simba, Mwangu - 2004 - Application of ICT in strengthening health information systems in developing countries in the wake of globalisati.pdf:application/pdf}
}

@article{spiegelhalter_bayesian_1993,
	title = {Bayesian {Analysis} in {Expert} {Systems}},
	volume = {8},
	doi = {10.1214/ss/1177010888},
	abstract = {We review recent developments in applying Bayesian probabilistic and{\textbackslash}nstatistical ideas to expert systems. Using a real, moderately complex,{\textbackslash}nmedical example we illustrate how qualitative and quantitative knowledge{\textbackslash}ncan be represented within a directed graphical model, generally known{\textbackslash}nas a belief network in this context. Exact probabilistic inference{\textbackslash}non individual cases is possible using a general propagation procedure.{\textbackslash}nWhen data on a series of cases are available, Bayesian statistical{\textbackslash}ntechniques can be used for updating the original subjective quantitative{\textbackslash}ninputs, and we present a set of diagnostics for identifying conflicts{\textbackslash}nbetween the data and the prior specification. A model comparison{\textbackslash}nprocedure is explored, and a number of links made with mainstream{\textbackslash}nstatistical methods. Details are given on the use of Dirichlet prior{\textbackslash}ndistributions for learning about parameters and the process of transforming{\textbackslash}nthe original graphical model to a junction tree as the basis for{\textbackslash}nefficient computation.},
	number = {3},
	journal = {Statistical Science},
	author = {Spiegelhalter, David and Dawid, Philip and Lauritzen, Steffen and Cowell, Robert},
	year = {1993},
	pages = {219--247},
	file = {Spiegelhalter et al. - 1993 - Bayesian Analysis in Expert Systems:/home/gregoire/.zotero/zotero/ttzhcdaf.default/zotero/storage/7KUP5I6S/Spiegelhalter et al. - 1993 - Bayesian Analysis in Expert Systems.pdf:application/pdf}
}

