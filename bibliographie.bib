@article{braa_improving_2012,
	title = {Improving quality and use of data through data-use workshops: {Zanzibar}, {United} {Republic} of {Tanzania}.},
	volume = {90},
	issn = {0042-9686},
	url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3341693&tool=pmcentrez&rendertype=abstract},
	doi = {10.2471/BLT.11.099580},
	abstract = {PROBLEM: In Zanzibar, United Republic of Tanzania, as in many developing countries, health managers lack faith in the national Health Management Information System (HMIS). The establishment of parallel data collection systems generates a vicious cycle: national health data are used little because they are of poor quality, and their relative lack of use, in turn, makes their quality remain poor.{\textbackslash}n{\textbackslash}nAPPROACH: An action research approach was applied to strengthen the use of information and improve data quality in Zanzibar. The underlying premise was that encouraging use in small incremental steps could help to break the vicious cycle and improve the HMIS.{\textbackslash}n{\textbackslash}nLOCAL SETTING: To test the hypothesis at the national and district levels a project to strengthen the HMIS was established in Zanzibar. The project included quarterly data-use workshops during which district staff assessed their own routine data and critiqued their colleagues' data.{\textbackslash}n{\textbackslash}nRELEVANT CHANGES: The data-use workshops generated inputs that were used by District Health Information Software developers to improve the tool. The HMIS, which initially covered only primary care outpatients and antenatal care, eventually grew to encompass all major health programmes and district and referral hospitals. The workshops directly contributed to improvements in data coverage, data set quality and rationalization, and local use of target indicators.{\textbackslash}n{\textbackslash}nLESSONS LEARNT: Data-use workshops with active engagement of data users themselves can improve health information systems overall and enhance staff capacity for information use, presentation and analysis for decision-making.},
	number = {5},
	journal = {Bulletin of the World Health Organization},
	author = {Braa, J√∏rn and Heywood, Arthur and Sahay, Sundeep},
	year = {2012},
	keywords = {Data Collection, Decision Making, Developing Countries, Hospital Information Systems, Tanzania},
	pages = {379--84},
	file = {Braa, Heywood, Sahay - 2012 - Improving quality and use of data through data-use workshops Zanzibar, United Republic of Tanzania:/home/gregoire/.zotero/zotero/ttzhcdaf.default/zotero/storage/AS7MEHNQ/Braa, Heywood, Sahay - 2012 - Improving quality and use of data through data-use workshops Zanzibar, United Republic of Tanzania.pdf:application/pdf}
