@article{sandefur_political_2013,
	title = {The {Political} {Economy} of {Bad} {Data}: {Evidence} from {African} {Survey} \& {Administrative} {Statistics}},
	volume = {51},
	url = {http://www1.wider.unu.edu/inclusivegrowth/sites/default/files/IGA/Sandefur.pdf},
	doi = {10.1080/00220388.2014.968138},
	abstract = {Administrative data across multiple African countries on health, education, agriculture, and poverty systematically exaggerate progress relative to independent surveys. We nd evidence for two explanations for these discrepancies: political interference and weak state capacity. Evidence of political interference arises when aid donors pay for results (e.g., immunization rates across 41 countries) or statistics are politically salient (e.g., in ation in Cameroon). In contrast, central governments are themselves misled when civil servants reporting data are responsible for measured progress (e.g., agricultural production in Tanzania) or data determines funding allocations (e.g., school enrollment across 21 countries). These two phenomena suggest distinct policy agendas.},
	number = {2},
	journal = {The Journal of Development Studies},
	author = {Sandefur, Justin and Glassman, Amanda},
	year = {2013},
	keywords = {administrative data, Africa, HMIS, household surveys, Immunization, national statistics systems},
